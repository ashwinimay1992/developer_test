{
  "name": "eprompto-ITAM",
  "version": "2.0.4",
  "description": "To test AutoUpdate in ITAM",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashwinimay1992/developer_test.git"
  },
  "build":{
    "appId": "itam_apiId_enduser",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "images/ePrompto.ico",
      "requestedExecutionLevel": "requireAdministrator"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "auto-launch": "^5.0.5",
    "child_process": "^1.0.2",
    "csvtojson": "^2.0.10",
    "electron": "^11.0.2",
    "electron-builder": "^22.9.1",
    "electron-log": "^4.3.0",
    "fs": "0.0.1-security",
    "ip": "^1.1.5",
    "jquery": "^3.5.1",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "node-disk-info": "^1.1.0",
    "node-machine-id": "^1.1.12",
    "node-os-utils": "^1.3.2",
    "request": "^2.88.2",
    "select2": "^4.0.13",
    "serial-number": "^1.3.0",
    "systeminformation": "^4.30.1"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "child_process": "^1.0.2",
    "csvtojson": "^2.0.10",
    "electron-updater": "^4.3.5",
    "electron-log": "^4.3.0",
    "fs": "0.0.1-security",
    "ip": "^1.1.5",
    "jquery": "^3.5.1",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "node-disk-info": "^1.1.0",
    "node-machine-id": "^1.1.12",
    "node-os-utils": "^1.3.2",
    "request": "^2.88.2",
    "select2": "^4.0.13",
    "serial-number": "^1.3.0",
    "systeminformation": "^4.30.1"
  }
}
